{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 2,
    "react/jsx-filename-extension": [ 1, {"extensions": [".js", ".jsx"]} ],
    "react/destructuring-assignment": [ "error", "always", {"ignoreClassFields": true} ],
    "prettier/prettier": [ "error", {"semi": true} ]
  }
}